{
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "NETCORE_SUPPORT", "NETCORE", "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "NETCORE_SUPPORT", "NETCORE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "NUnitLite": "3.5.0"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Collections.Specialized": "4.*",
        "System.Reflection.Extensions": "4.*",
        "System.ComponentModel.TypeConverter": "4.*",
        "System.ServiceModel.Primitives": "4.*",
        "System.Linq.Queryable": "4.*",
        "System.Threading": "4.*",
        "System.Threading.Thread": "4.*",
        "System.Threading.ThreadPool": "4.*",
        "System.Threading.Tasks": "4.*",
        "System.Data.SqlClient": "4.*",
        "AWSSDK.Core": "3.3.4.1",
        "AWSSDK.DynamoDBv2": "3.3.0.2",
        "AWSSDK.S3": "3.3.1.1",
        "AWSSDK.SQS": "3.3.0.2",
        "ServiceStack.Text": "1.0.*",
        "ServiceStack.Interfaces": "1.0.*",
        "ServiceStack.Client": "1.0.*",
        "ServiceStack.Common": "1.0.*",
        "ServiceStack": "1.0.*",
        "ServiceStack.Aws": "1.0.*"
      }
    }
  }
}