{
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "NETCORE_SUPPORT", "NETCORE", "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "NETCORE_SUPPORT", "NETCORE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "NUnitLite": "3.5.0"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "System.Data.Common": "4.*",
        "AWSSDK.Core": "3.3.10.2",
        "AWSSDK.DynamoDBv2": "3.3.4.3",
        "AWSSDK.S3": "3.3.5.10",
        "AWSSDK.SQS": "3.3.1.10",
        "ServiceStack.Text": "1.0.*",
        "ServiceStack.Interfaces": "1.0.*",
        "ServiceStack.Client": "1.0.*",
        "ServiceStack.Common": "1.0.*",
        "ServiceStack": "1.0.*",
        "ServiceStack.Aws": "1.0.*"
      }
    }
  }
}